{
    "name": "helgesverre/prune",
    "description": "Add a Twig filter for CraftCMS templates to 'prune' out fields of entries, originally by Matt Stauffer, Ported to Craft CMS 3",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "weather"
    ],
    "support": {
        "docs": "https://github.com/helgesverre/prune/blob/master/README.md",
        "issues": "https://github.com/helgesverre/prune/issues"
    },
    "license": "MIT",
    "authors": [{
        "name": "Helge Sverre",
        "homepage": "https://helgesver.re"
    }],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "HelgeSverre\\Prune\\": "src/"
        }
    },
    "extra": {
        "name": "Prune",
        "handle": "prune",
        "schemaVersion": "1.0.0",
        "hasCpSettings": true,
        "hasCpSection": false,
        "class": "HelgeSverre\\Prune\\Prune"
    }
}
